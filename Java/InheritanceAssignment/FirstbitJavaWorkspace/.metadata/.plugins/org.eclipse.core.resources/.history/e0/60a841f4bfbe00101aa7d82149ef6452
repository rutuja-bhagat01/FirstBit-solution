package com.firstbit.inheritance;

class Employee {
	
	int id;
	String name;
	double salary;
	
	Employee()
	{
		System.out.println("Employee default constructor called...");
		id = 131;
		name = "Not given";
		salary = 30000;
	}
	
	Employee(int id, String name, double salary) {
		super();
		this.id = id;
		this.name = name;
		this.salary = salary;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getSalary() {
		return salary;
	}

	public void setSalary(double salary) {
		this.salary = salary;
	}
	
}//class Employee ends here


