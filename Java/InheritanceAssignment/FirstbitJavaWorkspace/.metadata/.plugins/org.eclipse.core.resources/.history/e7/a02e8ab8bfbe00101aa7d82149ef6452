package com.firstbit.inheritance;

class Employee {
	
	int id;
	String name;
	double salary;
	
	Employee()
	{
		System.out.println("Employee default constructor called...");
		id = 131;
		name = "Not given";
		salary = 30000;
	}
	
	Employee(int id, String name, double salary) {
		super();
		this.id = id;
		this.name = name;
		this.salary = salary;
	}
	
	
}
